{
  "automock": false,
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.js"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/__tests__/"
  ],
  "coverageReporters": [
    "json"
  ],
  "testMatch": [
    "**/test/**/pushWithStorybookFlag.test.js"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/test/resources/"
  ],
  "testEnvironment": "node",
  "reporters": [
    "default",
    "jest-junit"
  ]
}
